{
    "deepscan.enable": true,
    "docwriter.language": "English",
    "docwriter.hotkey.windows": "Alt + .",
    "cSpell.words": [
        "damodar",
        "eren",
        "mikasa",
        "myfirstfunction",
        "pwskills",
        "sudh"
